#
# File currently only used by code located in manage/
# Repeats parameters already present in config.yml
# Apart from code located in manage/ g5 code only uses config.yml
#

# ---------------------------------------------------------------------------------------------------
# DB connection
# ---------------------------------------------------------------------------------------------------
DB_HOST=localhost
DB_PORT=5432
DB_USER=g5_pg_username
DB_PASSWORD=g5_pg_password
DB_DBNAME=g5_pg_dbname
DB_SCHEMA=g5_pg_schema

# ---------------------------------------------------------------------------------------------------
# Backup database
# ---------------------------------------------------------------------------------------------------
PGDUMP_CMD=pg_dump
PGDUMP_DIR=/path/to/dir/containing/dbdumps
